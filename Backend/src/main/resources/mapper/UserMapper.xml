<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.EduCity.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.EduCity.model.entity.User">
            <id property="email" column="email" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="citymap" column="cityMap" jdbcType="BIGINT"/>
            <result property="isactive" column="isActive" jdbcType="TINYINT"/>
            <result property="activecode" column="activeCode" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        email,name,password,
        cityMap,isActive,activeCode,
        avatar
    </sql>
</mapper>
